/*
 * File: countDown.c 
 * -----------------
 * This program prints the message T minus n 
 * and counting, where n is supplied when the function is called.  
 */

#include <stdio.h>
 
/* Function prototypes */

void printCount (int n ); 

/* Main program */

int main (void) {
   int i;
  
   for (i = 36; i > 0; --i) {
      printCount(i);  /* Calling  function */ 
   }
   return 0;
}

/* 
 * Function: printCount
 * Usage: int printCount (i);
 * --------------------------
 * Prints  countdown.
 */

void printCount(int n ) {
   printf("T minus %d and counting\n", n);
}






 
