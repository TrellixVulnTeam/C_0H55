#include <stdio.h>

/* Function prototypes */

void testauto(); 

/* Main program */

int main() {
   int count; /* create the auto variable count */
   for (count = 0; count <= 12; count++)
      testauto(); /* Calling the function */

   return 0;
}

void testauto() {
  int num = 7; /* create the auto variable num */
               /* and initialize to zero */

  printf("The value of the automatic variable num is %d\n", num);
  num++;
}
