/*
 * File: pgm3-4.c 
 * ---------------
 * This program illustrates the use of increment operator.
 */


#include <stdio.h>


int main()
{
   int count;
  
   printf("\nThe initial value of count is %d.", count);
   count++;
   printf("\n count is now %d.", count);
   count++;
   printf("\n count is now %d.", count);
   count++;
   printf("\n count is now %d.", count);
   count++;
   printf("\n count is now %d.", count);

