/*
 * File: dummy2.c 
 * -------------
 * This program illustrates simple array operations.
 */


#include <stdio.h>

/* Constants */

#define SIZE 5


/* Main program */

int main() {
  int i a[5]= {2, 5, 6, 7, 12};
   float a[5], b[5];
   
   for (i = 0; i < 5; i++) {
      a[i] = i * 10.5;
      b[i]= a[i] + 45.0;
      printf("%f   %f\n", a[i],  b[i]);
   }
   return 0;
}
